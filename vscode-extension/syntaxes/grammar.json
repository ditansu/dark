{
	"name": "Darklang grammar",
	"scopeName": "source.darklang",
	"foldingStartMarker": "",
	"foldingStopMarker": "",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#chars"
		},
		{
			"include": "#double_tick"
		},
		{
			"include": "#definition"
		},
		{
			"include": "#abstract_definition"
		},
		{
			"include": "#attributes"
		},
		{
			"include": "#modules"
		},
		{
			"include": "#anonymous_functions"
		},
		{
			"include": "#du_declaration"
		},
		{
			"include": "#record_declaration"
		},
		{
			"include": "#records"
		},
		{
			"include": "#strp_inlined"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#text"
		}
	],
	"repository": {
		"strp_inlined_body": {
			"patterns": [
				{
					"include": "#comments"
				},
				{
					"include": "#anonymous_functions"
				},
				{
					"match": "(\\^[[:alpha:]0-9'._]+)",
					"captures": {
						"1": {
							"name": "entity.name.type.darklang"
						}
					}
				},
				{
					"name": "keyword.darklang",
					"match": "\\b(and|when|or)\\b"
				},
				{
					"begin": "(\\()",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"end": "(\\))",
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"include": "#strp_inlined_body"
						}
					]
				},
				{
					"match": "(static member|member)\\s*([[:alpha:]0-9'`<>^._]+|``[[:alpha:]0-9' <>^._]+``)\\s*(:)",
					"captures": {
						"1": {
							"name": "keyword.darklang"
						},
						"2": {
							"name": "variable.darklang"
						},
						"3": {
							"name": "keyword.symbol.darklang"
						}
					}
				},
				{
					"include": "#constants"
				},
				{
					"include": "#strings"
				},
				{
					"include": "#chars"
				},
				{
					"include": "#double_tick"
				},
				{
					"include": "#keywords"
				},
				{
					"include": "#text"
				},
				{
					"include": "#definition"
				},
				{
					"include": "#attributes"
				},
				{
					"include": "#keywords"
				},
				{
					"include": "#text"
				}
			]
		},
		"strp_inlined": {
			"patterns": [
				{
					"begin": "(\\()",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"end": "(\\))",
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"include": "#strp_inlined_body"
						}
					]
				}
			]
		},
		"generic_declaration": {
			"patterns": [
				{
					"comments": "SRTP syntax support",
					"begin": "(:)\\s*(\\()\\s*(static member|member)",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "keyword.symbol.darklang"
						},
						"3": {
							"name": "keyword.darklang"
						}
					},
					"end": "(\\))",
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"begin": "(\\()",
							"beginCaptures": {
								"1": {
									"name": "keyword.symbol.darklang"
								}
							},
							"end": "(\\))",
							"endCaptures": {
								"1": {
									"name": "keyword.symbol.darklang"
								}
							},
							"patterns": [
								{
									"include": "#member_declaration"
								}
							]
						},
						{
							"match": "(('|\\^)[[:alpha:]0-9'._]+)",
							"captures": {
								"1": {
									"name": "entity.name.type.darklang"
								}
							}
						},
						{
							"include": "#variables"
						},
						{
							"include": "#keywords"
						}
					]
				},
				{
					"name": "keyword.darklang",
					"match": "\\b(private|to|public|internal|function|yield!|yield|class|exception|match|delegate|of|new|in|as|if|then|else|elif|for|begin|end|inherit|do|let\\!|return\\!|return|interface|with|abstract|enum|member|try|finally|and|when|or|use|use\\!|struct|while|mutable|assert|base|done|downcast|downto|extern|fixed|global|lazy|upcast|not)(?!')\\b"
				},
				{
					"name": "keyword.symbol.darklang",
					"match": ":"
				},
				{
					"include": "#constants"
				},
				{
					"match": "(('|\\^)[[:alpha:]0-9'._]+)",
					"captures": {
						"1": {
							"name": "entity.name.type.darklang"
						}
					}
				},
				{
					"begin": "(<)",
					"end": "(>)",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"match": "(('|\\^)[[:alpha:]0-9'._]+)",
							"captures": {
								"1": {
									"name": "entity.name.type.darklang"
								}
							}
						},
						{
							"include": "#tuple_signature"
						},
						{
							"include": "#generic_declaration"
						}
					]
				},
				{
					"begin": "(\\()",
					"end": "(\\))",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"match": "(([?[:alpha:]0-9'`^._ ]+))+",
							"captures": {
								"1": {
									"name": "entity.name.type.darklang"
								}
							}
						},
						{
							"include": "#tuple_signature"
						}
					]
				},
				{
					"match": "(?!when|and|or\\b)\\b([\\w0-9'`^._]+)",
					"comments": "Here we need the \\w modifier in order to check that the words are allowed",
					"captures": {
						"1": {
							"name": "entity.name.type.darklang"
						}
					}
				},
				{
					"match": "(\\|)",
					"comments": "Prevent captures of `|>` as a keyword when defining custom operator like `<|>`",
					"captures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					}
				},
				{
					"include": "#keywords"
				}
			]
		},
		"anonymous_record_declaration": {
			"begin": "(\\{\\|)",
			"end": "(\\|\\})",
			"beginCaptures": {
				"1": {
					"name": "keyword.symbol.darklang"
				}
			},
			"endCaptures": {
				"1": {
					"name": "keyword.symbol.darklang"
				}
			},
			"patterns": [
				{
					"match": "[[:alpha:]0-9'`^_ ]+(:)",
					"captures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					}
				},
				{
					"match": "([[:alpha:]0-9'`^_ ]+)",
					"captures": {
						"1": {
							"name": "entity.name.type.darklang"
						}
					}
				},
				{
					"include": "#anonymous_record_declaration"
				},
				{
					"include": "#keywords"
				}
			]
		},
		"record_signature": {
			"patterns": [
				{
					"match": "[[:alpha:]0-9'`^_ ]+(=)([[:alpha:]0-9'`^_ ]+)",
					"captures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "variable.parameter.darklang"
						}
					}
				},
				{
					"begin": "({)",
					"end": "(})",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"match": "[[:alpha:]0-9'`^_ ]+(=)([[:alpha:]0-9'`^_ ]+)",
							"captures": {
								"1": {
									"name": "keyword.symbol.darklang"
								},
								"2": {
									"name": "variable.parameter.darklang"
								}
							}
						},
						{
							"include": "#record_signature"
						}
					]
				},
				{
					"include": "#keywords"
				}
			]
		},
		"tuple_signature": {
			"patterns": [
				{
					"match": "(([?[:alpha:]0-9'`^._ ]+))+",
					"captures": {
						"1": {
							"name": "entity.name.type.darklang"
						}
					}
				},
				{
					"begin": "(\\()",
					"end": "(\\))",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"match": "(([?[:alpha:]0-9'`^._ ]+))+",
							"captures": {
								"1": {
									"name": "entity.name.type.darklang"
								}
							}
						},
						{
							"include": "#tuple_signature"
						}
					]
				},
				{
					"include": "#keywords"
				}
			]
		},
		"anonymous_functions": {
			"patterns": [
				{
					"name": "function.anonymous",
					"begin": "\\b(fun)\\b",
					"end": "(->)",
					"beginCaptures": {
						"1": {
							"name": "keyword.darklang"
						}
					},
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.arrow.darklang"
						}
					},
					"patterns": [
						{
							"include": "#comments"
						},
						{
							"begin": "(\\()",
							"end": "\\s*(?=(->))",
							"beginCaptures": {
								"1": {
									"name": "keyword.symbol.darklang"
								}
							},
							"endCaptures": {
								"1": {
									"name": "keyword.symbol.arrow.darklang"
								}
							},
							"patterns": [
								{
									"include": "#member_declaration"
								}
							]
						},
						{
							"include": "#variables"
						}
					]
				}
			]
		},
		"attributes": {
			"patterns": [
				{
					"name": "support.function.attribute.darklang",
					"begin": "\\[\\<",
					"end": "\\>\\]|\\]",
					"patterns": [
						{
							"include": "$self"
						}
					]
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"name": "comment.literate.command.darklang",
					"match": "(\\(\\*{3}.*\\*{3}\\))",
					"beginCaptures": {
						"1": {
							"name": "comment.block.darklang"
						}
					}
				},
				{
					"name": "comment.block.markdown.darklang",
					"begin": "^\\s*(\\(\\*\\*(?!\\)))((?!\\*\\)).)*$",
					"while": "^(?!\\s*(\\*)+\\)\\s*$)",
					"beginCaptures": {
						"1": {
							"name": "comment.block.darklang"
						}
					},
					"endCaptures": {
						"1": {
							"name": "comment.block.darklang"
						}
					},
					"patterns": [
						{
							"include": "text.html.markdown"
						}
					]
				},
				{
					"name": "comment.block.darklang",
					"begin": "(\\(\\*(?!\\)))",
					"end": "(\\*+\\))",
					"beginCaptures": {
						"1": {
							"name": "comment.block.darklang"
						}
					},
					"endCaptures": {
						"1": {
							"name": "comment.block.darklang"
						}
					},
					"patterns": [
						{
							"comments": "Capture // when inside of (* *) like that the rule which capture comments starting by // is not trigger. See https://github.com/ionide/ionide-fsgrammar/issues/155",
							"name": "fast-capture.comment.line.double-slash.darklang",
							"match": "//"
						},
						{
							"include": "#comments"
						}
					]
				},
				{
					"name": "comment.block.markdown.darklang.end",
					"match": "((?<!\\()(\\*)+\\))",
					"captures": {
						"1": {
							"name": "comment.block.darklang"
						}
					}
				},
				{
					"name": "comment.line.markdown.darklang",
					"begin": "///",
					"while": "///",
					"patterns": [
						{
							"include": "text.html.markdown"
						}
					]
				},
				{
					"name": "comment.line.double-slash.darklang",
					"match": "//.*$"
				}
			]
		},
		"constants": {
			"patterns": [
				{
					"name": "keyword.symbol.darklang",
					"match": "\\(\\)"
				},
				{
					"name": "constant.numeric.float.darklang",
					"match": "\\b-?[0-9][0-9_]*((\\.([0-9][0-9_]*([eE][+-]??[0-9][0-9_]*)?)?)|([eE][+-]??[0-9][0-9_]*))"
				},
				{
					"name": "constant.numeric.integer.nativeint.darklang",
					"match": "\\b(-?((0(x|X)[0-9a-fA-F][0-9a-fA-F_]*)|(0(o|O)[0-7][0-7_]*)|(0(b|B)[01][01_]*)|([0-9][0-9_]*)))"
				},
				{
					"name": "constant.language.boolean.darklang",
					"match": "\\b(true|false)\\b"
				},
				{
					"name": "constant.other.darklang",
					"match": "\\b(null|void)\\b"
				}
			]
		},
		"abstract_definition": {
			"name": "abstract.definition.darklang",
			"begin": "\\b(abstract)\\s+(member)?(\\s+\\[\\<.*\\>\\])?\\s*([_[:alpha:]0-9,\\._`\\s]+)(:)",
			"end": "\\s*(with)\\b|=|$",
			"beginCaptures": {
				"1": {
					"name": "keyword.darklang"
				},
				"2": {
					"name": "keyword.darklang"
				},
				"3": {
					"name": "support.function.attribute.darklang"
				},
				"5": {
					"name": "keyword.symbol.darklang"
				}
			},
			"endCaptures": {
				"1": {
					"name": "keyword.darklang"
				}
			},
			"patterns": [
				{
					"include": "#comments"
				},
				{
					"include": "#common_declaration"
				},
				{
					"match": "(\\?{0,1})([[:alpha:]0-9'`^._ ]+)\\s*(:)((?!with\\b)\\b([\\w0-9'`^._ ]+)){0,1}",
					"captures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "variable.parameter.darklang"
						},
						"3": {
							"name": "keyword.symbol.darklang"
						},
						"4": {
							"name": "entity.name.type.darklang"
						}
					}
				},
				{
					"match": "(?!with|get|set\\b)\\b([\\w0-9'`^._]+)",
					"comments": "Here we need the \\w modifier in order to check that the words isn't blacklisted",
					"captures": {
						"1": {
							"name": "entity.name.type.darklang"
						}
					}
				},
				{
					"include": "#keywords"
				}
			]
		},
		"common_binding_definition": {
			"patterns": [
				{
					"include": "#comments"
				},
				{
					"include": "#attributes"
				},
				{
					"comments": "SRTP syntax support",
					"begin": "(:)\\s*(\\()\\s*(static member|member)",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "keyword.symbol.darklang"
						},
						"3": {
							"name": "keyword.darklang"
						}
					},
					"end": "(\\))\\s*((?=,)|(?=\\=))",
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"match": "(\\^[[:alpha:]0-9'._]+)",
							"captures": {
								"1": {
									"name": "entity.name.type.darklang"
								}
							}
						},
						{
							"include": "#variables"
						},
						{
							"include": "#keywords"
						}
					]
				},
				{
					"begin": "(:)\\s*(\\()",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "keyword.symbol.darklang"
						}
					},
					"end": "(\\)\\s*(([?[:alpha:]0-9'`^._ ]*)))",
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "entity.name.type.darklang"
						}
					},
					"patterns": [
						{
							"include": "#tuple_signature"
						}
					]
				},
				{
					"begin": "(:)\\s*(\\^[[:alpha:]0-9'._]+)\\s*(when)",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "entity.name.type.darklang"
						},
						"3": {
							"name": "keyword.darklang"
						}
					},
					"end": "(?=:)",
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"name": "keyword.darklang",
							"match": "\\b(and|when|or)\\b"
						},
						{
							"comment": "Because we first capture the keywords, we can capture what looks like a word and assume it's an entity definition",
							"match": "([[:alpha:]0-9'^._]+)",
							"captures": {
								"1": {
									"name": "entity.name.type.darklang"
								}
							}
						},
						{
							"name": "keyword.symbol.darklang",
							"match": "(\\(|\\))"
						}
					]
				},
				{
					"match": "(:)\\s*([?[:alpha:]0-9'`^._ ]+)",
					"captures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "entity.name.type.darklang"
						}
					}
				},
				{
					"match": "(->)\\s*(\\()?\\s*([?[:alpha:]0-9'`^._ ]+)*",
					"captures": {
						"1": {
							"name": "keyword.symbol.arrow.darklang"
						},
						"2": {
							"name": "keyword.symbol.darklang"
						},
						"3": {
							"name": "entity.name.type.darklang"
						}
					}
				},
				{
					"begin": "(\\*)\\s*(\\()",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "keyword.symbol.darklang"
						}
					},
					"end": "(\\)\\s*(([?[:alpha:]0-9'`^._ ]+))+)",
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "entity.name.type.darklang"
						}
					},
					"patterns": [
						{
							"include": "#tuple_signature"
						}
					]
				},
				{
					"begin": "(\\*)(\\s*([?[:alpha:]0-9'`^._ ]+))*",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						},
						"2": {
							"name": "entity.name.type.darklang"
						}
					},
					"end": "(?==)|(?=\\))",
					"endCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
					},
					"patterns": [
						{
							"include": "#tuple_signature"
						}
					]
				},
				{
					"begin": "(<+(?![[:space:]]*\\)))",
					"beginComment": "The group (?![[:space:]]*\\) is for protection against overload operator. static member (<)",
					"end": "((?<!:)>|\\))",
					"endComment": "The group (?<!:) prevent us from stopping on :> when using SRTP synthax",
					"beginCaptures": {
						"1": {
							"name": "keyword.symbol.darklang"
						}
